<!--this will be shown for both /search and inside the turbo-frame with
id="search_results"-->
<turbo-frame id="<%= params.fetch(:turbo_frame, "search_results") %>">
  <ul>
    <% @stations.each do |station| %>
      <li>
        <%= link_to "#{station.water_body_name} (#{station.name})", station_path(station) %>
      </li>
    <% end %>
  </ul>
</turbo-frame>

